# Buat tabel baru
dbExecute(conn, "
CREATE TABLE customer_survey (
Date DATE,
Customer_ID TEXT,
Age INTEGER,
Gender TEXT,
Location TEXT,
Delivery_Time INTEGER,
Product_Quality INTEGER,
Customer_Service TEXT,
Satisfaction_Score TEXT,
Recommendation TEXT,
Complaint TEXT,
Salary INTEGER,
Total_Spending INTEGER,
Discount INTEGER,
How_Heard TEXT,
Usage_Frequency TEXT,
Most_Valued_Feature TEXT,
Website_Ease INTEGER,
Packaging_Satisfactory TEXT,
Repeat_Purchase TEXT,
Price_Competitiveness INTEGER,
Issues_Experienced TEXT,
Suggestions TEXT,
Overall_Satisfaction INTEGER
)
")
# Daftar opsi saran
suggestion_options <- c(
"Better product quality",
"Faster delivery",
"Improved customer service",
"More discount offers",
"User-friendly website",
"Better packaging",
"More product variety",
"Clearer product information",
"More payment options",
"Other"
)
# Buat data dummy
set.seed(123)
n <- 500
dummy_data <- data.frame(
Date = sample(seq(as.Date('2023-01-01'), as.Date('2024-12-31'), by = "day"), n, replace = TRUE),
Customer_ID = paste0("C", sprintf("%04d", 1:n)),
Age = sample(18:65, n, replace = TRUE),
Gender = sample(c("Male", "Female"), n, replace = TRUE),
Location = sample(c("Jakarta", "Bandung", "Surabaya", "Medan", "Makassar"), n, replace = TRUE),
Delivery_Time = sample(1:7, n, replace = TRUE),
Product_Quality = sample(1:5, n, replace = TRUE),
Customer_Service = sapply(1:n, function(i) paste(sample(1:5, sample(1:2, 1)), collapse = ", ")),
Satisfaction_Score = sapply(1:n, function(i) paste(sample(1:10, sample(1:3, 1)), collapse = ", ")),
Recommendation = sample(c("Yes", "No"), n, replace = TRUE),
Complaint = sample(c("", "Late delivery", "Damaged item", "Poor service", "Wrong product"), n, replace = TRUE),
Salary = sample(seq(1000000, 30000000, by = 1000000), n, replace = TRUE),
Total_Spending = sample(seq(1000000, 30000000, by = 1000000), n, replace = TRUE),
Discount = sample(10:50, n, replace = TRUE),
How_Heard = sample(c("Online Ads", "Friends/Family", "Social Media", "Search Engine", "Other"), n, replace = TRUE),
Usage_Frequency = sample(c("Daily", "Weekly", "Monthly", "Rarely", "First time"), n, replace = TRUE),
Most_Valued_Feature = sample(c("Quality", "Price", "Design", "Brand", "Durability"), n, replace = TRUE),
Website_Ease = sample(1:5, n, replace = TRUE),
Packaging_Satisfactory = sample(c("Yes", "No"), n, replace = TRUE),
Repeat_Purchase = sample(c("Yes", "No"), n, replace = TRUE),
Price_Competitiveness = sample(1:5, n, replace = TRUE),
Issues_Experienced = sample(c("Yes", "No"), n, replace = TRUE),
Suggestions = sapply(1:n, function(i) paste(sample(suggestion_options, sample(1:3, 1)), collapse = ", ")),
Overall_Satisfaction = sample(1:10, n, replace = TRUE),
stringsAsFactors = FALSE
)
# Simpan data ke database
dbWriteTable(conn, "customer_survey", dummy_data, append = TRUE)
